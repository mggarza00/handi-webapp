{
  "name": "homaid-webapp",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mggarza00/homaid-webapp.git"
  },
  "homepage": "https://github.com/mggarza00/homaid-webapp",
  "bugs": {
    "url": "https://github.com/mggarza00/homaid-webapp/issues"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.css": [
      "stylelint --fix",
      "prettier --write"
    ],
    "*.{md,mdx,json,yml,yaml}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "dev": "next dev",
    "dev:onedrive": "cross-env WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=1000 CHOKIDAR_USEPOLLING=1 next dev",
    "clean": "rimraf .next",
    "dev:clean": "npm run clean && npm run dev:onedrive",
    "clean:full": "rimraf .next .turbo node_modules && npm ci",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:css": "stylelint \"**/*.css\"",
    "lint": "next lint && npm run lint:css",
    "lint:admin": "eslint app/admin/layout.tsx app/admin/page.tsx app/admin/offers/**/*.tsx app/admin/requests/**/*.tsx app/admin/professionals/**/*.tsx app/admin/clients/**/*.tsx app/admin/payments/**/*.tsx app/admin/settings/**/*.tsx app/admin/system/**/*.tsx app/admin/[...stub]/page.tsx app/api/admin/metrics/**/*.ts app/api/admin/requests/**/*.ts app/api/admin/professionals/**/*.ts app/api/admin/clients/**/*.ts app/api/admin/payments/**/*.ts app/api/admin/webhooks/**/*.ts app/api/admin/audit/**/*.ts app/api/admin/settings/**/*.ts app/api/admin/offers/**/*.ts components/admin/**/*.tsx components/RoleGate.tsx components/ui/table.tsx hooks/use-admin-auth.ts --max-warnings=0",
    "check:admin": "tsc -p tsconfig.json --noEmit && npm run lint:admin",
    "review:ui": "node scripts/ui-review.mjs",
    "check": "npm run typecheck && npm run lint",
    "smoke": "node scripts/smoke.mjs",
    "prepare": "husky install",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ui": "playwright test --ui",
    "snap": "node scripts/snapshots.js",
    "poster": "node scripts/extract-poster.mjs",
    "ui:revise": "tsx tools/ui-revision.ts",
    "ui:revise:target": "tsx tools/ui-revision.ts --target=",
    "classify:gen": "tsx scripts/generate-heuristics.ts",
    "format": "prettier --write .",
    "test:e2e:headed": "playwright test --headed",
    "seed:reviews-jobs": "curl -s -X POST http://localhost:3000/api/dev/seed-reviews-jobs || true",
    "dev:seed:reviews": "cross-env ENABLE_DEV_SEED=1 tsx ./scripts/call-seed-reviews-jobs.ts",
    "verify:reviews-jobs": "node scripts/verify-reviews-jobs.mjs",
    "verify:supabase": "node scripts/verify-supabase-wrappers.mjs",
    "check:supabase-clients": "node scripts/check-supabase-client-usage.cjs",
    "test:e2e:ci": "playwright test -c playwright.ci.config.ts",
    "db:link": "supabase link",
    "db:gen": "supabase gen types --lang=typescript --project-id $env:SUPABASE_PROJECT_ID > types/supabase.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset --force",
    "db:studio": "supabase studio",
    "db:seed:admin": "supabase db execute --file supabase/seed_admin.sql",
    "db:seed": "supabase db execute --file db/seed/seed.sql",
    "db:seed:large": "supabase db execute --file supabase/seed_admin_large.sql",
    "db:setup:admin": "npm run db:migrate && npm run db:seed:admin",
    "dev:seed": "tsx scripts/dev-seed.ts",
    "test": "npm run test:e2e",
    "test:unit": "vitest --config vitest.config.ts run --reporter=verbose",
    "e2e": "playwright test -c playwright.local.config.ts",
    "e2e:ui": "playwright test --ui -c playwright.local.config.ts",
    "db:push:direct": "bash scripts/db_push_direct.sh",
    "db:receipt:psql": "bash scripts/psql_apply_receipt_pooler.sh",
    "db:push:ipv6": "bash scripts/db_push_ipv6.sh",
    "db:receipt:cli": "supabase migration up --linked",
    "supabase:doctor": "bash scripts/supabase/install-and-doctor.sh",
    "supabase:up": "bash scripts/supabase/dev-up.sh",
    "supabase:down": "bash scripts/supabase/dev-down.sh",
    "supabase:reset": "bash scripts/supabase/dev-reset.sh",
    "supabase:link-update": "bash scripts/supabase/link-update.sh",
    "supa:install": "bash scripts/supabase/install-and-doctor.sh",
    "supa:start": "bash scripts/supabase/dev-up.sh",
    "supa:stop": "bash scripts/supabase/dev-down.sh",
    "supa:reset": "bash scripts/supabase/dev-reset.sh",
    "supa:link": "bash scripts/supabase/link-update.sh",
    "supa:lint": "supabase db lint",
    "supa:push": "npx -y supabase@latest db push",
    "supa:status": "supabase status",
    "supa:studio": "supabase studio",
    "preview:email:accepted": "tsx scripts/preview-accepted-email.ts",
    "deploy:vercel": "node scripts/deploy-vercel.mjs",
    "audit:assets": "node scripts/audit-large-assets.mjs",
    "audit:raw": "node scripts/audit-raw-strings.mjs",
    "audit:webpack": "npm run -s audit:assets && npm run -s audit:raw"
  },
  "dependencies": {
    "@fontsource/inter": "^5.2.8",
    "@gsap/react": "^2.1.2",
    "@hookform/resolvers": "^3.10.0",
    "@mapbox/mapbox-gl-geocoder": "^5.1.2",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-pdf/renderer": "^4.3.1",
    "@resvg/resvg-js": "^2.6.2",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/auth-helpers-shared": "^0.7.0",
    "@supabase/auth-js": "^2.80.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.80.0",
    "@tanstack/react-table": "^8.21.3",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "ffmpeg-static": "^5.2.0",
    "framer-motion": "^11.11.1",
    "googleapis": "^131.0.0",
    "gsap": "^3.13.0",
    "leaflet": "^1.9.4",
    "lenis": "^1.1.14",
    "lucide-react": "^0.539.0",
    "mapbox-gl": "^3.7.0",
    "mapbox-gl-geocoder": "2.0.1",
    "motion": "^12.23.24",
    "next": "14.2.10",
    "next-themes": "^0.4.6",
    "ogl": "^1.0.11",
    "qrcode": "^1.5.4",
    "react": "^18.2.0",
    "react-datepicker": "^8.7.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.64.0",
    "react-leaflet": "^4.2.1",
    "recharts": "^3.3.0",
    "resend": "^6.0.1",
    "satori": "^0.18.3",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "web-push": "^3.6.7",
    "zod": "^3.25.76",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.21",
    "dotenv": "^16.4.5",
    "eslint": "^8.57.1",
    "eslint-config-next": "14.2.10",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "execa": "^9.6.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "playwright": "^1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.14.0",
    "pwa-asset-generator": "^8.1.2",
    "rimraf": "^5.0.5",
    "serve": "^14.2.4",
    "sharp": "^0.33.5",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "supabase": "^2.54.11",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.19.0",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {},
      "autoprefixer": {}
    }
  },
  "overrides": {},
  "browserslist": [
    "defaults",
    "ios_saf >= 10",
    "safari >= 10"
  ]
}
