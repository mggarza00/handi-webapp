{
  "name": "handi-webapp",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/mggarza00/handi-webapp.git"
  },
  "homepage": "https://github.com/mggarza00/handi-webapp",
  "bugs": {
    "url": "https://github.com/mggarza00/handi-webapp/issues"
  },
  "scripts": {
    "dev": "next dev",
    "dev:onedrive": "cross-env WATCHPACK_POLLING=true WATCHPACK_POLLING_INTERVAL=1000 CHOKIDAR_USEPOLLING=1 next dev",
    "clean": "rimraf .next",
    "dev:clean": "npm run clean && npm run dev:onedrive",
    "clean:full": "rimraf .next .turbo node_modules && npm ci",
    "build": "next build",
    "start": "next start",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint:css": "stylelint \"**/*.css\"",
    "lint": "next lint && npm run lint:css",
    "review:ui": "node scripts/ui-review.mjs",
    "check": "npm run typecheck && npm run lint",
    "smoke": "node scripts/smoke.mjs",
    "prepare": "husky install",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:ui": "playwright test --ui",
    "snap": "node scripts/snapshots.js",
    "format": "prettier --write .",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "playwright test --reporter=line",
    "db:link": "supabase link",
    "db:gen": "supabase gen types --lang=typescript --project-id $env:SUPABASE_PROJECT_ID > types/supabase.ts",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset --force",
    "db:studio": "supabase studio",
    "test:unit": "vitest --config vitest.config.ts run --reporter=verbose"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/auth-helpers-nextjs": "^0.10.0",
    "@supabase/ssr": "^0.5.2",
    "@supabase/supabase-js": "^2.57.4",
    "browser-image-compression": "^2.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "googleapis": "^131.0.0",
    "lucide-react": "^0.539.0",
    "next": "^14.2.10",
    "next-themes": "^0.4.6",
    "react": "^18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "^18.2.0",
    "resend": "^6.0.1",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@lhci/cli": "^0.15.1",
    "@playwright/test": "^1.55.0",
    "@prisma/client": "^6.14.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.19.11",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.2.10",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^4.0.0-beta.0",
    "husky": "^9.1.7",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "prisma": "^6.14.0",
    "rimraf": "^5.0.5",
    "serve": "^14.2.4",
    "stylelint": "^16.23.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-config-tailwindcss": "^1.0.0",
    "supabase": "^2.39.2",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "postcss": {
    "plugins": {
      "@tailwindcss/postcss": {},
      "autoprefixer": {}
    }
  },
  "overrides": {}
}