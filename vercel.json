{
  "redirects": [
    {
      "source": "/(.*)",
      "has": [{ "type": "host", "value": "www.handi.mx" }],
      "destination": "https://handi.mx/$1",
      "permanent": true
    }
  ],
  "rewrites": [
    { "source": "/icons/favicon.png", "destination": "/icons/Favicon v1 png.png" },
    { "source": "/favicon.ico", "destination": "/icons/Favicon v1 png.png" }
  ]
}
